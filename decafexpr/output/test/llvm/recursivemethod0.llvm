; ModuleID = 'Test'

declare void @print_int(i32)

define void @test(i32) {
entry:
  %y = alloca i32
  store i32 undef, i32* %y
  %x = load i32, i32* %x
  %y1 = load i32, i32* %y
  %AddTmp = add i32 %x, %y1
  call void @test(i32 %AddTmp)
  store i32 %0, i32* %x
  ret void
}

define i32 @main() {
entry:
  %x = alloca i32
  store i32 1, i32* %x
  %x1 = load i32, i32* %x
  call void @test(i32 %x1)
  ret i32 0
}
