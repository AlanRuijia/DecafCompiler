extern func print_string(string) void;
extern func read_int() int;

package pro
{
	func main() int
	{
		var len int;
		var i int;
		while(true)
		{
			len=read_int();
			if(len==0)
			{
				return (0);
			}
			for(i=0; i<len; i=i+1)
			{
				print_string("*");
			}
			print_string("\n");
		}
		return (0);
	}
}