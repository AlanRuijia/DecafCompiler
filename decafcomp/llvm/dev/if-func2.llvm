; ModuleID = 'C'

declare void @print_int(i32)

define i32 @f() {
entry:
  ret i32 1
  ret i32 0
}

define void @main() {
entry:
  %x = alloca i32
  store i32 0, i32* %x
  %calltmp = call i32 @f()
  %IfCond = icmp eq i32 %calltmp, 0
  br i1 %IfCond, label %Else, label %Then

Then:                                             ; preds = %entry
  store i32 1, i32* %x
  br label %IfCont

Else:                                             ; preds = %entry
  store i32 2, i32* %x
  br label %IfCont

IfCont:                                           ; preds = %Else, %Then
  %x1 = load i32, i32* %x
  call void @print_int(i32 %x1)
  ret void
}
