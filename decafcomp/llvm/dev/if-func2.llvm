; ModuleID = 'C'

declare void @print_int(i32)

define i1 @f() {
entry:
  call void @print_int(i32 5)
  ret i1 true
  ret i1 true
}

define void @main() {
entry:
  %x = alloca i32
  store i32 0, i32* %x
  %calltmp = call i1 @f()
  %IfCond = icmp eq i1 %calltmp, true
  br i1 %IfCond, label %Then, label %Else

Then:                                             ; preds = %entry
  store i32 1, i32* %x
  br label %IfCont

Else:                                             ; preds = %entry
  store i32 2, i32* %x
  br label %IfCont

IfCont:                                           ; preds = %Else, %Then
  %x1 = load i32, i32* %x
  call void @print_int(i32 %x1)
  ret void
}
