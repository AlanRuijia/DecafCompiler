; ModuleID = 'Test'

declare void @print_int(i32)

define i32 @main() {
entry:
  %x = alloca i1
  store i1 false, i1* %x
  %y = alloca i1
  store i1 false, i1* %y
  %z = alloca i1
  store i1 false, i1* %z
  store i1 true, i1* %x
  store i1 true, i1* %y
  %x1 = load i1, i1* %x
  %LHS = icmp eq i1 %x1, true
  %LHS2 = icmp eq i1 %x1, true
  br i1 %LHS2, label %True, label %False

True:                                             ; preds = %entry
  %y3 = load i1, i1* %y
  br label %BooleanShortCircuit

False:                                            ; preds = %entry
  br label %BooleanShortCircuit

BooleanShortCircuit:                              ; preds = %False, %True
  %phival = phi i1 [ %y3, %True ], [ false, %False ]
  store i1 %phival, i1* %z
  %z4 = load i1, i1* %z
  %zexttmp = zext i1 %z4 to i32
  call void @print_int(i32 %zexttmp)
  ret i32 0
}
